<UserControl x:Class="NolowaFrontend.Controls.MainManuToggleButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NolowaFrontend.Controls"
             mc:Ignorable="d" Name="_this"
             d:DesignHeight="450" d:DesignWidth="800">
    <local:ToggleImageButton ImageHeight="25" ImageWitdh="25" Command="{Binding ElementName=_this, Path=Command}"
                             DefaultImageUri="{Binding ElementName=_this, Path=DefaultImageUri}"
                             ClickedImageUri="{Binding ElementName=_this, Path=ClickedImageUri}"/>
</UserControl>
